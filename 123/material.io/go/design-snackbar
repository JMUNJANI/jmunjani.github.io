    aria-labelledby="sr-lightbox-title">
  <h2 class="screenreader-only sr-lightbox-title" tabindex="0">
    Explore how Shrine, a retail app, uses Material Design.
  </h2>

    <header class="lightbox__header">
      <div class="lightbox__header-title">
        <span
            class="lightbox__button js-lightbox-close"
            data-comp="Tooltip"
            data-props-tooltip='{"tip": "Close", "direction": "bottom"}'>
          <i
              class="material-icons notranslate material-icons--padded mdc-ripple-surface"
              data-mdc-ripple-is-unbounded tabindex="0">
            close
          </i>
        </span>
        <h2 class="lightbox__title js-lightbox-title"></h2>
